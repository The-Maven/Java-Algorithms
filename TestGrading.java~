public class TestGrading
{
  public static void main(String[] args)
  {
    System.out.println(Grading.tellGrade(-20));    // main method to test grades : including extremes and border values
    System.out.println(Grading.tellGrade(90));
    System.out.println(Grading.tellGrade(80));
    System.out.println(Grading.tellGrade(77));
    System.out.println(Grading.tellGrade(68));
    System.out.println(Grading.tellGrade(75));
    System.out.println(Grading.tellGrade(60));
    System.out.println(Grading.tellGrade(200));
    System.out.println(Grading.tellGrade(45));
    System.out.println(Grading.tellGrade(50));
    System.out.println(Grading.tellGrade(63.9));
  }
}

